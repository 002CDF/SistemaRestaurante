<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name = "datos.MesaCompuesta" table = "mesacompuesta">
<id column = "idMesaCompuesta" name = "idMesaCompuesta">
<generator class = "identity"/>
</id> 
<set name="mesasSimples" table="mesasimple" order-by="idMesaSimple asc"
	inverse="true" lazy="true" fetch="select">
	<key column="idMesaCompuesta" not-null="true" />
	<one-to-many class="datos.MesaSimple" />
</set>
</class>
</hibernate-mapping>