<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name = "datos.SubRubro" table = "subrubro">
<id column = "idSubRubro" name = "idSubRubro">
<generator class = "identity"/>
</id>
<property column = "descripcion" name = "descripcion" type = "string"/>
<set name="productos" table="producto" order-by="idProducto asc"
	inverse="true" lazy="true" fetch="select">
	<key column="idSubRubro" not-null="true" />
	<one-to-many class="datos.Producto" />
</set>
<set table="itemrubro" name="rubros" outer-join="true">
	<key column="idSubRubro" />
	<many-to-many column="idRubro" class="datos.Rubro"/>
</set>
</class>
</hibernate-mapping>